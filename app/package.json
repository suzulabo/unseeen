{
  "name": "unseeen-app",
  "private": true,
  "scripts": {
    "srcvar": "echo $(git branch --show-current)/$(git rev-parse --short HEAD)$([ \"$(git status --porcelain)\" ] && echo '+')",
    "build": "_SRCVAR=$(npm --silent run srcvar) stencil build",
    "start": "_SRCVAR=$(npm --silent run srcvar) stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "test.spec2": "jest --config jest-spec2.config.js",
    "test.spec2.watch": "jest --config jest-spec2.config.js --watchAll",
    "generate": "stencil generate",
    "terms": "marked -o src/assets/terms.html -i terms.md"
  },
  "devDependencies": {
    "@peculiar/webcrypto": "^1.1.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@stencil/core": "^1.17.3",
    "@stencil/router": "^1.0.1",
    "@stencil/sass": "^1.3.2",
    "@stencil/store": "^1.3.0",
    "@types/ed2curve": "^0.2.2",
    "@types/jest": "25.2.3",
    "@types/puppeteer": "2.0.1",
    "ed2curve": "^0.3.0",
    "fake-indexeddb": "^3.1.0",
    "firebase": "^7.16.0",
    "idb": "^5.0.4",
    "ionicons": "^5.1.2",
    "jest": "26.0.1",
    "jest-cli": "26.0.1",
    "marked": "^1.1.1",
    "puppeteer": "2.1.1",
    "ts-jest": "^26.1.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-stencil": "^1.0.1",
    "type-fest": "^0.15.1",
    "workbox-build": "4.3.1"
  }
}
